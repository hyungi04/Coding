#include <iostream>
using namespace std;
class cc {
public:
    cc() { a = 2; b = 3; c = 4;
    cout << ii++ << " " << this << endl;
    };
    float a,b,c;
    static int ii;
};
int cc::ii = 0;

int main() {
    cc c1, c2[2], c3, *c4, c5, *c6;
    c4 = new cc[10];
    c6 = new cc[3];
    cout << cc::ii << endl; 
}
