#include <iostream>
using namespace std;

class mm {
public:
    mm() { a = 4 + 300; cout << "mm\n"; }
    mm(int b) { a = b + 100; cout << "mmb\n"; }
    int a;
};

class nn : public mm {
public:
    nn() { a = 11; cout << "nn\n"; }
    nn(int b) : mm(b) { c = a; a = b; cout << "nnb\n"; }
    int c;
};

class oo : public nn {
public:
    oo() { d = a; cout << "oo\n"; }
    oo(int b) : nn(b) { d = a; cout << "oob\n"; }
    int d;
};

int main() {
    nn pp(5);
    cout << pp.a << endl;
    cout << pp.c << endl;

    oo qq(7);
    cout << qq.a << endl;
    cout << qq.c << endl;
    cout << qq.d << endl;

    return 123;
}
